{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.6",
    "@mui/lab": "^5.0.0-alpha.162",
    "@mui/material": "^5.15.6",
    "@mui/x-data-grid": "^6.19.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "env-cmd -f ./environments/development.env react-app-rewired start",
    "build": "env-cmd -f ./environments/production.env react-app-rewired build",
    "test": "env-cmd -f ./environments/test.env react-app-rewired test",
    "eject": "react-scripts eject",
    "prod": "serve -s build",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/",
    "lint:fix": "eslint --fix --ext .jsx,.js,.tsx,.ts src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "babel-plugin-direct-import": "^1.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "customize-cra": "^1.0.0",
    "env-cmd": "^10.1.0",
    "react-app-rewired": "^2.2.1",
    "redux-logger": "^3.0.6",
    "sass": "^1.70.0",
    "serve": "^14.2.1"
